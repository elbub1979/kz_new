<h2><%= t('.sign_up') %></h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field my-1">
    <%= f.label t('.fname') %><br/>
    <%= f.text_field :fname, autofocus: true, autocomplete: "fname" %>
  </div>

  <div class="field my-1">
    <%= f.label t('.pname') %><br/>
    <%= f.text_field :pname, autofocus: true, autocomplete: "pname" %>
  </div>

  <div class="field my-1">
    <%= f.label t('.lname') %><br/>
    <%= f.text_field :lname, autofocus: true, autocomplete: "lname" %>
  </div>

  <div class="field my-1">
    <%= f.label t('.email') %><br/>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field my-1">
    <div>
      <%= f.label t('.password') %>
      <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> <%= t('.characters_minimum') %>)</em>
      <% end %><br/>
    </div>
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field my-1">
    <%= f.label t('.password_confirmation') %><br/>
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <div class="actions my-1">
    <%= f.submit t('.sign_up') %>
  </div>
<% end %>

<%= link_to 'qwe', read_from_ad_path%>

<%= render "devise/shared/links" %>
